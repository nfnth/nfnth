          for (let a = 0; a < traits.length; a++) {
            switch(traits[a].trait_type.toLowerCase()) {
              case 'name': tmp[14] = traits[a].value.toString(); break;
              case 'coord': tmp[12] = traits[a].value.toString(); break;
